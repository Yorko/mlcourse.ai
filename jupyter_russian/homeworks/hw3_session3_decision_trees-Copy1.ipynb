   "source": [
    "X1, Y1 = [x for x in X if x<-1.5], [y for x,y in zip(X,y) if x<-1.5]\n",
    "X2, Y2 = [x for x in X if x>=-1.5 and x<0], [y for x,y in zip(X,y) if x>=-1.5 and x<0]\n",
    "X3, Y3 = [x for x in X if x>=0 and x<1.5], [y for x,y in zip(X,y) if x>=0 and x<1.5]\n",
    "X4, Y4 = [x for x in X if x>1.5], [y for x,y in zip(X,y) if x>1.5]\n",
    "    \n",
    "reg_tree_pred = np.append([np.mean(Y1) for x in X1], [np.mean(Y2) for x in X2])\n",
    "reg_tree_pred = np.append(reg_tree_pred ,[np.mean(Y3) for x in X3])\n",
    "reg_tree_pred = np.append(reg_tree_pred ,[np.mean(Y4) for x in X4])\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.plot(X, y, \"b\")\n",
    "plt.scatter(X, y, c=\"b\", s=20)\n",
    "plt.plot(X, reg_tree_pred, \"g\", lw=2)\n",
    "plt.xlim([-2, 2])\n",
    "plt.title(\"Tree with max_depth=0\")\n",
    "plt.show()"
   ]
